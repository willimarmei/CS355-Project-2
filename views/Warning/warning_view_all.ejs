<!DOCTYPE html>
<html>
<head>
    <title> Warning - View All</title>
</head>
    <body>
        <h1>Warnings</h1>

        <table border = "1">
            <tr>
                <th>Medication ID</th>
                <th>Warning</th>
            </tr>
            <% for(var i = 0; i < warning.length; i++) {%>
                <tr>
                    <td><%=warning[i].medication_id %></td>
                    <td><%=warning[i].warning %></td>

                    <td>
                        <a href="/warning/edit/?medication_id/<%=warning[i].medication_id%>">
                            Edit
                        </a>
                    </td>
                </tr>
            <% } %>
        </table>
    </body>
</html>